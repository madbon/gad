{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "kartik-v/yii2-tabs-x": "@dev",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "miloschuman/yii2-highcharts-widget": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-widget-timepicker": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-export": "@dev",
        "kartik-v/yii2-mpdf": "dev-master",
        "c006/yii2-migration-utility": "dev-master",
        "phpoffice/phpspreadsheet": "dev-master",
        "miloschuman/yii2-highcharts-widget": "dev-master",
        "kartik-v/yii2-checkbox-x": "@dev",
        "kartik-v/yii2-money": "dev-master",
        "wbraganca/yii2-dynamicform": "2.0.1",
        "niksko12/yii2-user": "dev-master",
        "niksko12/yii2-rbac": "dev-master",
        "niksko12/yii2-trees-graphs": "dev-master",
        "niksko12/yii2-audit-log": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "2.4.0",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        },
        "secure-http":false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/niksko12/yii2-user.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/niksko12/yii2-rbac.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/niksko12/yii2-trees-graphs.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/niksko12/yii2-audit-log.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/akesandiego/data-dictionary-module.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/akesandiego/monitoring.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/akesandiego/single-sign-on-authentication.git"
        },
        {
            "type": "vcs",
            "url": "http://git.istms/akesandiego/yii2-attachments.git"
        }

    ],
	"extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
	}
}
